{
  "expo": {
    "name": "beko",
    "slug": "beko",
    "version": "1.0.0",
    "scheme": "beko",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-video",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1036338973947-dpf6mgoahom2r6e9qm2flsbog29c8me0"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android": true,
          "ios": true,
          "androidAppId": "ca-app-pub-3099985704584658~7990797006",
          "iosAppId": "ca-app-pub-3099985704584658~4313880641"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "사진을 선택하기 위해 접근 권한이 필요합니다.",
        "NSCameraUsageDescription": "사진을 찍기 위해 카메라 접근 권한이 필요합니다."
      },
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.ordihong.beko",
      "usesNonExemptEncryption": false,
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1036338973947-dpfemgoahom2r6e9qm2flsbog29c8me0"
        },
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~3347511713"
      }
    },
    "android": {
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~3347511713"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ordihong.beko",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "beko"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fd62690b-c7b1-4850-a014-8fd3746a89ea"
      }
    },
    "owner": "ordihong"
  }
}
